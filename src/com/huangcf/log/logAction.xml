<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
	"http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="/log" namespace="/log" extends="webhcf">
		<action name="showLog" class="com.huangcf.log.action.LogAction" method="showLog">
			<result type="json">
				<param name="root">jsonData</param> 
			</result>
		</action>
		<action name="writeLog" class="com.huangcf.log.action.LogAction" method="writeLog">
			<result type="json">
				<param name="root">jsonData</param> 
			</result>
		</action>
	</package>
</struts>