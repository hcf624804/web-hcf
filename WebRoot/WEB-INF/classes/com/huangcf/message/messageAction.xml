<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
	"http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="/message" namespace="/message" extends="webhcf">
		<action name="queryMessage" class="com.huangcf.message.action.MessageAction" method="queryMessage">
			<result type="json">
				<param name="root">jsonData</param> 
			</result>
		</action>
		<action name="writeMessage" class="com.huangcf.message.action.MessageAction" method="writeMessage">
			<result type="json">
				<param name="root">jsonData</param> 
			</result>
		</action>
	</package>
</struts>